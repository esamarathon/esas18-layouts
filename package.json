{
  "name": "esas18-layouts",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esamarathon/esas18-layouts.git"
  },
  "author": "zoton2",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/esamarathon/esas18-layouts/issues"
  },
  "homepage": "https://github.com/esamarathon/esas18-layouts#readme",
  "nodecg": {
    "compatibleRange": "^1.0.0",
    "dependencies": [
      "speedcontrol/nodecg-speedcontrol:~0.9"
    ],
    "graphics": [
      {
        "file": "host-dashboard.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "omnibar.html",
        "width": 1920,
        "height": 80
      },
      {
        "file": "transition.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "sponsor-video.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": true
      },
      {
        "file": "intermission.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "intermission-hosts.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "game-layout.html",
        "width": 1920,
        "height": 1080
      }
    ],
    "dashboardPanels": [
      {
        "name": "game-layout-override",
        "title": "Game Layout Override",
        "width": 2,
        "file": "game-layout-override.html",
        "workspace": "Scene Editing"
      },
      {
        "name": "sponsor-video-control",
        "title": "Sponsor Video Control",
        "width": 3,
        "file": "sponsor-video-control.html"
      },
      {
        "name": "music-player",
        "title": "Music Player",
        "width": 2,
        "file": "music-player.html"
      }
    ],
    "assetCategories": [
      {
        "name": "sponsor-logos",
        "title": "Sponsor Logos",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "png",
          "svg"
        ]
      },
      {
        "name": "sponsor-videos",
        "title": "Sponsor Videos",
        "allowedTypes": [
          "mp4"
        ]
      },
      {
        "name": "sponsor-slides",
        "title": "Sponsor Slide Images/Videos",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "png",
          "svg",
          "mp4"
        ]
      }
    ]
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "clone": "^2.1.1",
    "css": "^2.2.3",
    "moment": "^2.22.2",
    "mpd": "^1.3.0",
    "obs-websocket-js": "^1.2.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0"
  }
}
